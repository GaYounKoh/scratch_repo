   "source": [
    "# while문이 정말 3번 도는 가에 대한 의문을 풀기 위한 실행.\n",
    "n = 1\n",
    "sum = 0\n",
    "while n<=3:\n",
    "    print('n', n)\n",
    "    print('sum', sum)\n",
    "    sum = sum+n\n",
    "    n = n+1\n",
    "print('최종 n', n)\n",
    "print('최종 sum', sum)"
   ]
